import java.util.*;
public class Palindrome{ 
public static void main(String args[])
{
    int n=141;
    int temp=n;
    int r,sum=0;
    while(n>0) {
        r=n%10;
        sum=sum*10+r;
        n=n/10;
    }
    if(temp==sum) {
        System.out.println("Palindrome number");
    }
    else
    {
        System.out.println("Not an Palindrome number");
    }
}
}
